<script type="text/javascript" src="js/zepto.min.js"></script>
<script type="text/javascript">
$(function(){
var navi = navigator.userAgent;
	if(navi.indexOf("android")!=-1||navi.indexOf("ios")!=-1){
		$(".header_l").hide();
	}
$(".fitems_left li").click(function(){
var a=$(this).attr("id");
$(this).css("border","1px solid #dddcdc");
$(this).siblings().css("border","none");
$(this).siblings().find("img").remove();
$(this).append("<img src='images/arrow_fenlei.png' />");
$("."+a).css("display","block");
$("."+a).siblings().css("display","none");
})
})
/*头部搜索点击关闭或者弹出搜索框*/  
function showSearch(){
	document.getElementById("search_box").style.display="block";
}
function closeSearch(){
	document.getElementById("search_box").style.display="none";
}
/* 搜索验证 */
function check(Id){
	var strings = document.getElementById(Id).value;
	if(strings.replace(/(^\s*)|(\s*$)/g, "").length == 0){
		return false;
	}
	return true;
}


</script>
