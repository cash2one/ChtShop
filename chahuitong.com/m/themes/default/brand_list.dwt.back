<script type="text/javascript">
  $(function(){
  $.noConflict();
    var max=7;
    var min=1;
    $(".waterfallCon").swipeLeft(function(){
      var tem=$(".sideOn");
      var side=tem.next(".side");
      var num=side.attr("id");
      if(num==undefined){
        return false;
      }
      $(".top").removeClass("on");
      $(".slideBar1").children(".top").eq(num-1).addClass("on");
      side.addClass("sideOn");
      tem.removeClass("sideOn");
      if(num>=4){
        jQuery("#slideBar11").animate({scrollLeft:"200"},1000);
      }
    })
    
    $(".waterfallCon").swipeRight(function(){
      var tem=$(".sideOn");
      var side=tem.prev(".side");
      var num=side.attr("id");
      if(num==undefined){
        return false;
      }
      $(".top").removeClass("on");
      $(".slideBar1").children(".top").eq(num-1).addClass("on");
      side.addClass("sideOn");
      tem.removeClass("sideOn");
      if(num<=4){
        jQuery("#slideBar11").animate({scrollLeft:"0"},1000);
      }
    })
  })
  jQuery(".top").click(function(){
    var number=$(this).attr("id");
    $(".on").removeClass("on");
    $(".sideOn").hide();
    $(this).addClass("on");
    $(".effect-"+number).addClass("sideOn");
    $(".effect-"+number).show();
  })
  var navi = navigator.userAgent;
  if(navi.indexOf("android")!=-1||navi.indexOf("ios")!=-1){
    jQuery("#header").hide();
    jQuery(".waterfallCon").css("margin-top","0px");
  }
</script>

